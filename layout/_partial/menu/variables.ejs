<%
suka_share_icon = 'https://theme.suka.moe/demo/img/suka-favicon.png';

var relative = theme.head.favicon.ico.indexOf('http') == -1;

function suka_share_img_url_for(url) {
    if (relative === true) {
        return config.url + url_for(url).replace(config.root, '/');
    } else {
        return url;
    }
}

if (theme.head.favicon.large) {
    suka_share_icon = suka_share_img_url_for(theme.head.favicon.large);
} else if (theme.head.favicon.apple_touch_icon) {
    suka_share_icon = suka_share_img_url_for(theme.head.favicon.apple_touch_icon);
} else if (theme.head.favicon.medium) {
    suka_share_icon = suka_share_img_url_for(theme.head.favicon.medium);
} else if (theme.head.favicon.small) {
    suka_share_icon = suka_share_img_url_for(theme.head.favicon.small)
} else if (theme.head.favicon.ico) {
    suka_share_icon = suka_share_img_url_for(theme.head.favicon.ico);
}

%>